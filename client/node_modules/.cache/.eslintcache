lStorage.href = \"/\";\n  };\n\n  const adminRouter = () => {\n    return (\n      <>\n        <li>\n          <Link to=\"/create_product\">Create Products</Link>\n        </li>\n        <li>\n          <Link to=\"/category\">Categories</Link>\n        </li>\n      </>\n    );\n  };\n\n  const loggedRouter = () => {\n    return (\n      <>\n        <li>\n          <Link to=\"/history\">History</Link>\n        </li>\n        <li>\n          <Link to=\"/\" onClick={logoutUser}>\n            Logout\n          </Link>\n        </li>\n      </>\n    );\n  };\n\n  return (\n    <div>\n      <header>\n        <div className=\"menu\">\n          <img src={Menu} alt=\"\" width=\"30\" />\n        </div>\n\n        <div className=\"logo\">\n          <h1>\n            <Link to=\"/\">{isAdmin ? \"Admin\" : \"Sneaker Shop\"} </Link>\n          </h1>\n        </div>\n\n        <ul>\n          <li>\n            <Link to=\"/\">{isAdmin ? \"Products\" : \"Shop\"} </Link>\n          </li>\n\n          {isAdmin && adminRouter()}\n          {isLogged ? (\n            loggedRouter()\n          ) : (\n            <li>\n              <Link to=\"/login\">Login âœ¥ Register</Link>\n            </li>\n          )}\n\n          <li>\n            <img src={Exit} alt=\"\" width=\"30\" className=\"menu\" />\n          </li>\n        </ul>\n\n        {isAdmin ? (\n          \"\"\n        ) : (\n          <div className=\"cart-icon\">\n            <span>{cart.length}</span>\n            <Link to=\"/cart\">\n              <img src={Cart} alt=\"\" width=\"30\" />\n            </Link>\n          </div>\n        )}\n      </header>\n    </div>\n  );\n}\n\nexport default Header;\n","/Users/weifeng/Desktop/SneakerShop/client/src/components/mainpages/Pages.js",["86","87"],"import React from 'react'\nimport { BrowserRouter, Routes, Route, Link } from 'react-router-dom';\nimport Products from '../mainpages/products/Products';\nimport Login from './auth/Login';\nimport Register from './auth/Register';\nimpo