/ console.log('render test')\n\n        if (params.id) {\n            products.forEach(product => {\n                if (product._id === params.id) setDetailProduct(product)\n            });\n        }\n    }, [params, products])\n\n    console.log(detailProduct)\n    if (detailProduct.length === 0) return null;\n\n    return (\n        <>\n            <div className=\"detail\">\n                <img src={detailProduct.images.url} alt=\"\" />\n                <div className='box-detail'>\n                    <div className='row'>\n                        <h2>{detailProduct.title}</h2>\n                        <h6>#id: {detailProduct.product_id}</h6>\n                    </div>\n\n                    <span>${detailProduct.price}</span>\n                    <p>{detailProduct.description}</p>\n                    <p>{detailProduct.content}</p>\n                    <p>Sold: {detailProduct.sold}</p>\n                    <Link to=\"/cart\" className='cart'>Buy Now</Link>\n\n                </div>\n            </div>\n\n            <div>\n                <h2>Releated Products</h2>\n                <div className='products'>\n                    {\n                        products.map(product => {\n                            return product.category === detailProduct.category\n                                ? <ProductItem key={product._id} product={product} /> : null\n                        })\n                    }\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default DetailProduct;\n","/Users/weifeng/Desktop/SneakerShop/client/src/components/mainpages/utils/productItem/BtnRender.js",[],"/Users/weifeng/Desktop/SneakerShop/client/src/components/mainpages/utils/loading/Loading.js",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"92","replacedBy":"96"},{"ruleId":"94","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":1,"column":17,"nodeType":"100","messageId":"101","endLine":1,"endColumn":25},